{
	"name": "emseapi",
	"version": "0.1.2",
	"private": true,
	"scripts": {
		"start": "nest start",
		"start:prod": "node dist/src/main",
		"dev": "nest start --watch",
		"build": "yarn db:generate && nest build",
		"typings": "ts-node gql/generate-typings.ts",
		"generate": "yarn typings && yarn db:generate",
		"generate:watch": "concurrently -n \"types,schema\" -c \"bgBlue.bold,bgMagenta.bold\" \"export WATCH=\"true\" && yarn typings\" \"yarn db:generate --watch\"",
		"test": "vitest",
		"test:watch": "vitest --watch",
		"test:cov": "vitest --coverage",
		"db:generate": "npx prisma generate",
		"db:studio": "npx prisma studio --browser none --port 5555",
		"db:format": "npx prisma format",
		"db:validate": "npx prisma validate",
		"lint": "eslint --ext .ts .",
		"lint:fix": "eslint --ext .ts . --fix",
		"docs:gen": "spectaql ./docs.config.yml",
		"docs:watch": "spectaql -D ./docs.config.yml"
	},
	"dependencies": {
		"@nestjs/apollo": "^10.0.2",
		"@nestjs/cli": "^9.1.1",
		"@nestjs/common": "^8.0.6",
		"@nestjs/core": "^8.0.6",
		"@nestjs/graphql": "^10.0.8",
		"@nestjs/jwt": "^8.0.0",
		"@nestjs/passport": "^9.0.0",
		"@nestjs/platform-express": "^8.0.6",
		"@prisma/client": "^4.0.0",
		"@sentry/node": "^6.13.2",
		"@sentry/tracing": "^6.13.2",
		"@types/express": "^4.17.11",
		"@types/gapi": "^0.0.42",
		"apollo-server-express": "^3.3.0",
		"bcryptjs": "^2.4.3",
		"body-parser": "^1.19.0",
		"class-validator": "^0.13.1",
		"cookie-parser": "^1.4.5",
		"cors": "^2.8.5",
		"debug": "~2.6.9",
		"dotenv": "^8.2.0",
		"express": "~4.17.1",
		"express-session": "^1.17.1",
		"google-auth-library": "^8.5.1",
		"graphql": "^16.6.0",
		"graphql-iso-date": "^3.6.1",
		"http-errors": "^1.8.0",
		"jsonwebtoken": "^8.5.1",
		"moment": "^2.29.4",
		"ms": "^2.1.2",
		"passport": "^0.6.0",
		"passport-google-oauth20": "^2.0.0",
		"passport-jwt": "^4.0.0",
		"passport-local": "^1.0.0",
		"reflect-metadata": "^0.1.13",
		"rxjs": "^7.3.0",
		"source-map-support": "^0.5.21",
		"ts-morph": "^12.0.0",
		"type-graphql": "^1.1.1",
		"typescript": "^4.5.4"
	},
	"devDependencies": {
		"@babel/cli": "^7.8.4",
		"@nestjs/schematics": "^8.0.3",
		"@nestjs/testing": "^8.0.6",
		"@types/bcryptjs": "^2.4.2",
		"@types/cookie-parser": "^1.4.2",
		"@types/cors": "^2.8.10",
		"@types/graphql-iso-date": "^3.4.0",
		"@types/jest": "^28.1.7",
		"@types/jsonwebtoken": "^8.5.1",
		"@types/ms": "^0.7.31",
		"@vitest/coverage-c8": "^0.27.0",
		"@vitest/coverage-istanbul": "^0.27.0",
		"@vitest/ui": "^0.27.0",
		"babel-preset-env": "^1.7.0",
		"babel-preset-es2015": "^6.22.0",
		"babel-preset-stage-2": "^6.22.0",
		"babel-register": "^6.22.0",
		"concurrently": "^7.6.0",
		"eslint": "^7.24.0",
		"jest": "^28.1.3",
		"kill-port": "^1.6.1",
		"prettier": "^1.19.1",
		"prisma": "^4.0.0",
		"prisma-dbml-generator": "^0.9.1",
		"spectaql": "^2.0.3",
		"start-server-and-test": "^1.15.2",
		"ts-jest": "^28.0.8",
		"ts-node": "^10.9.1",
		"ts-node-dev": "^1.1.6",
		"tsconfig-paths": "^3.11.0",
		"vite-tsconfig-paths": "^4.0.3",
		"vitest": "^0.27.0"
	},
	"browserslist": [
		"> 0.25%",
		"not dead"
	]
}
